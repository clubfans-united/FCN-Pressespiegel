{
  "name": "clubfansunited/pressespiegel",
  "description": "WordPress Plugin FCN Pressespiegel",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Helmer",
      "email": "mail@clubfans-united.de",
      "homepage": "https://www.eracer.de"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": ">=8.2",
    "ozh/bookmarkletgen": "1.*",
    "rockschtar/wordpress-cronjob": "1.0.0",
    "laminas/laminas-feed": "2.*",
    "laminas/laminas-http": "2.*",
    "nesbot/carbon": "2.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.*",
    "kint-php/kint": "3.*"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  },
  "autoload": {
    "psr-4": {
      "FCNPressespiegel\\": "src/"
    }
  }
}
